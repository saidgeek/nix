{
  "editor.formatOnSave": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "editor.autoClosingQuotes": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": true,
  "editor.fontFamily": "VictorMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.guides.bracketPairs": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 120,
  "emmet.includeLanguages": {
    "astro": "html",
    "javascript": "javascriptreact",
    "svelte": "html",
    "typescript": "typescriptreact"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "nix.formatterPath": "nixpkgs-fmt",
  "nix.serverPath": "rnix-lsp",
  "security.workspace.trust.banner": "never",
  "svelte.enable-ts-plugin": true,
  "tailwindCSS.includeLanguages": {
    "astro": "html",
    "svelte": "html",
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  "terminal.integrated.fontFamily": "Noto Nerd Font",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 100000000,
  "terminal.integrated.tabs.location": "left",
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
    {
      "backgroundColor": "#D1663F",
      "color": "#ffffff",
      "overviewRulerColor": "gray",
      "text": "NOTE:"
    },
    {
      "backgroundColor": "#E9F02E",
      "color": "#232323",
      "text": "IDEA:"
    },
    {
      "backgroundColor": "#D1663F",
      "color": "#ffffff",
      "text": "REMOVE:"
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.dialogStyle": "custom",
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Palenight (Mild Contrast)",
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "editor.scrollbar.vertical": "hidden",
  "workbench.editor.showTabs": false,
  "workbench.iconTheme": "catppuccin-macchiato",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.glyphMargin": false,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true
}
